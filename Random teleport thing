-- Scripts:

local function UXCJEHC_fake_script() -- Loading.LocalScript 
	local script = Instance.new('LocalScript', Loading)

	local FLoading = script.Parent.Parent.Parent.FrameLoading
	local button = script.Parent
	local amount = 0
	local page1 = script.Parent.Parent.Parent["Page 1"]
	local GameName = game:GetService("MarketplaceService"):GetProductInfo(game.PlaceId).Name
	
	
	
	repeat
		button.Text = "Loading"
		wait(0.5)
		button.Text = "Loading."
		wait(0.5)
		button.Text = "Loading.."
		wait(0.5)
		button.Text = "Loading..."
		wait(0.5)
		amount += 1
		print(amount)
	until(amount == 3)
	
	
	if amount == 3 then
		button.Text = "Game Found!"
		wait(1)
		button.Text = GameName
		wait(2)
		button.Text = "Execute"
		button.MouseButton1Click:Connect(function()
			FLoading:Destroy()
			wait(0.2)
			page1.Visible = true
		end)
	end
end
coroutine.wrap(UXCJEHC_fake_script)()
local function XFOJ_fake_script() -- abcergegfdgfdgfdgdf.LocalScript 
	local script = Instance.new('LocalScript', abcergegfdgfdgfdgdf)

	repeat wait() until game.Players.LocalPlayer.Character
	
	local screengui = script.Parent
	local plr = game.Players.LocalPlayer
	local name = plr.Name
	local userid = plr.UserId
	local thumbType = Enum.ThumbnailType.AvatarBust
	local thumbSize = Enum.ThumbnailSize.Size420x420
	local content, isReady = game.Players:GetUserThumbnailAsync(userid, thumbType, thumbSize)
	local GameName = game:GetService("MarketplaceService"):GetProductInfo(game.PlaceId).Name
	local loading = script.Parent.FrameLoading
	local setting = script.Parent.settings
	local page1 = script.Parent["Page 1"]
	
	loading.ProfilePicture.Image = content
	loading.WelcomeBack.Text = "Welcome back "..name
	
	
	page1.Active = true
	page1.Draggable = true
	
	setting.Active = true
	setting.Draggable = true
	
	
	
	function onKeyPress(inputObject, gameProcessedEvent, istyping)
		if inputObject.KeyCode == Enum.KeyCode.RightShift and not istyping then
			if screengui.Enabled == false then
				screengui.Enabled = true
			else
				screengui.Enabled = false
			end
		end
	end
	game:GetService("UserInputService").InputBegan:connect(onKeyPress)
end
coroutine.wrap(XFOJ_fake_script)()
local function TIJM_fake_script() -- ScrollingFrame.LocalScript 
	local script = Instance.new('LocalScript', ScrollingFrame)

	local Plr = game:GetService("Players") 
	local PlrFold = script.Parent.Parent.ScrollingFrame
	
	local dummyButton = PlrFold.dummyButton:Clone() 
	PlrFold.dummyButton:Destroy() 
	
	function update() 
	
		PlrFold:ClearAllChildren() 
	
		local CurrentPlayer = 0 
	
		for i,v in pairs(Plr:GetPlayers()) do 
	
			CurrentPlayer = CurrentPlayer + 1 
	
			local newButton = dummyButton:Clone() 
	
			newButton.Position = UDim2.new( 0, 0, 0, newButton.Size.Y.Offset * ( CurrentPlayer - 1 ) ) 
			newButton.Text = v.Name 
	
			newButton.MouseButton1Down:connect(function()
				if Plr.LocalPlayer.Name == v.Name then
					newButton.Text = "Can't teleport to yourself"
				else
				Plr.LocalPlayer.Character.HumanoidRootPart.CFrame = v.Character.HumanoidRootPart.CFrame	--script
			end
			end)
	
			newButton.Parent = PlrFold
	
		end
	
	end
	
	Plr.PlayerAdded:connect(update) 
	Plr.PlayerRemoving:connect(update) 
	
	update()
end
coroutine.wrap(TIJM_fake_script)()
local function ITUSKE_fake_script() -- page1.LocalScript 
	local script = Instance.new('LocalScript', page1)

	local pages = script.Parent.Parent.Parent.Parent
	local page1 = pages["Page 1"]
	local setting = pages.settings
	local button  = script.Parent
	
	button.MouseButton1Click:Connect(function()
		page1.Visible = true
		setting.Visible = false
	end)
end
coroutine.wrap(ITUSKE_fake_script)()
