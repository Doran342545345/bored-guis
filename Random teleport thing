local function GCUE_fake_script() -- TextButton.LocalScript 
	local script = Instance.new('LocalScript', TextButton)

	local flashbang  = script.Parent.Parent.Parent.Flashbang
	local time = 0.5
	local button  = script.Parent
	
	button.MouseButton1Click:Connect(function()
		flashbang.Visible = true
		wait(time)
		flashbang.Visible = false
	end)
end
coroutine.wrap(GCUE_fake_script)()
